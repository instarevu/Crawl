<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 1.0;
        var shouldAutoPlay = false;
        var videoSizes = [[355, 292], [450, 370], [425, 349], [466, 383], [522, 429]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/81daPda5ezL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51zC7D9vGaL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/51zC7D9vGaL.jpg","main":{"http://ecx.images-amazon.com/images/I/81daPda5ezL._SX355_.jpg":[355,292],"http://ecx.images-amazon.com/images/I/81daPda5ezL._SX450_.jpg":[450,370],"http://ecx.images-amazon.com/images/I/81daPda5ezL._SX425_.jpg":[425,350],"http://ecx.images-amazon.com/images/I/81daPda5ezL._SX466_.jpg":[466,384],"http://ecx.images-amazon.com/images/I/81daPda5ezL._SX522_.jpg":[522,430]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81iq55KGzZL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51iM-6sk1%2BL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/51iM-6sk1%2BL.jpg","main":{"http://ecx.images-amazon.com/images/I/81iq55KGzZL._SX355_.jpg":[355,293],"http://ecx.images-amazon.com/images/I/81iq55KGzZL._SX450_.jpg":[450,371],"http://ecx.images-amazon.com/images/I/81iq55KGzZL._SX425_.jpg":[425,351],"http://ecx.images-amazon.com/images/I/81iq55KGzZL._SX466_.jpg":[466,385],"http://ecx.images-amazon.com/images/I/81iq55KGzZL._SX522_.jpg":[522,431]}},{"hiRes":"http://ecx.images-amazon.com/images/I/818x-iT3CEL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51vz88hL2KL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/51vz88hL2KL.jpg","main":{"http://ecx.images-amazon.com/images/I/818x-iT3CEL._SX355_.jpg":[355,286],"http://ecx.images-amazon.com/images/I/818x-iT3CEL._SX450_.jpg":[450,363],"http://ecx.images-amazon.com/images/I/818x-iT3CEL._SX425_.jpg":[425,343],"http://ecx.images-amazon.com/images/I/818x-iT3CEL._SX466_.jpg":[466,376],"http://ecx.images-amazon.com/images/I/818x-iT3CEL._SX522_.jpg":[522,421]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81%2BcwgnrY1L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41lujOPGg7L._SS40_.jpg","large":"http://ecx.images-amazon.com