<script type="text/javascript">
amznJQ.onCompletion("amznJQ.criticalFeature", function() {
amznJQ.available("popover", function() {
    var acr = jQuery("#averageCustomerReviews");
    var dest = "/gp/customer-reviews/common/du/displayHistoPopAjax.html?ref=" + acr.attr("data-ref") + 
               "&link=1&seeall=1&ASIN=" + acr.attr("data-asin");
    acr.find(".reviewCountTextLinkedHistogram").amazonPopoverTrigger({
        destination: dest,
        group: "reviewsPopover",
        hoverHideDelay:300,
        location:"bottom",
        locationAlign:"left",
        locationOffset:[-20,-2],
        paddingLeft: 15,
        paddingBottom: 5,
        paddingRight: 15,
        showOnHover:true,
        showCloseButton: true,
        width: null,
        onShow: function(popover) { 
            jQuery(document).click(); 
        }
    })
});
});
</script>




                    
    

</div>


        
        
    
        
        
        
        




<div id="ask_feature_div" class="feature" data-feature-name="ask">

    
        
        
            
            
                





  
    <span class="askPipe"> | </span>
  
  <span id="AskPopover" data-asin="B0071NO7I0"  popover-error-string="Unable to load. Please try again soon." popover-title="Ask the owners and get answers in email"> 
    <a id="AskPopover" class="a-link-normal" href="#Ask">
      <span class="a-size-base">
        9 answered questions
      </span>
    </a>
  </span>

  <style type="text/css">
    #averageCustomerReviews, #averageCustomerReviews_feature_div, #ask_feature_div  {
       display: inline-block;
       *display: inline;
    }
    .askPipe {
        color: #666666;
        padding: 0 5px 0 5px;
    }
  </style>
  
  <script type="text/javascript">
    function cdBindAskPopover() {
      jQuery('#AskPopover').each(function() {
        jQuery(this).removeAmazonPopoverTrigger();
        jQuery(this).amazonPopoverTrigger({
          ajaxTimeout: 4000,
          ajaxErrorContent: jQuery(this).attr('popover-error-string'),
          modal: true,
          width: 800,
          closeEventInclude: "MOUSE_LEAVE",
          closeEventExclude: ["MOUSE_LEAVE", "CLICK_OUTSIDE"],
          showCloseButton: true,
          showOnHover: false,
          title: jQuery(this).attr('popover-title'),
          cacheable: false,
          folllowScroll: true,
          destination: '/gp/product/features/popover/ask-popover.html?sourceWidget=AskATFDPX&asin=' + jQuery(this).attr('data-asin')
        });
      });
    }
  
    amznJQ.onReady('popover', cdBindAskPopover);
  </script>



                    
    

