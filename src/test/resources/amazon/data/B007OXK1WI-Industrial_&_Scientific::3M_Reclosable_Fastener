<script type="text/javascript">
    function attach_sns_popover_box(triggerName, divName) {
        amznJQ.available('jQuery', function() {
            var trigger = jQuery(triggerName);
            amznJQ.available('popover', function() {
                trigger.removeAmazonPopoverTrigger();
                trigger.amazonPopoverTrigger({
                      onShow: function(popover) {
                        popover.addClass('snsDpPopover');
                      },
                      title: " <div id=\"snsInfo_titleBar\" class=\"\"> <img src=\"http://g-ecx.images-amazon.com/images/G/01/x-locale/subscriptions/rcxsubs/dp/sns_icon_transparent._V396063411_.gif\" width=\"26\" alt=\"\" height=\"26\" border=\"0\" /> <span id=\"sns_popup_heading\"><strong><span style=\"color: #e47911; font-family: arial; font-size:18px\"><strong>Subscribe &amp; Save</strong></span></strong></span> </div> ",
                      width: 520,
                      location: 'centered',
                      localContent: divName,
                      draggable: true,
                      closeEventExclude: ['CLICK_TRIGGER'],
                      clone: true,
                      group: 'sns-details'
                      });
            });
        });
    }
</script>


