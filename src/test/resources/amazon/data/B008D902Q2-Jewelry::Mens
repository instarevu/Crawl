  <# jQuery.each(wishlist, function (i, item) { #>
    <li class='nav_pop_li'>
      <a href='<#=item.url #>' class='nav_a'>
        <#=item.name #>
      </a>
      <div class='nav_tag'>
        <# if(typeof item.count !='undefined') { #>
          <#=
            (item.count == 1 ? "{count} item" : "{count} items")
              .replace("{count}", item.count)
          #>
        <# } #>
      </div>
    </li>
  <# }); #>
</script>

  <script type='text/html' id='nav-tpl-cart'>
