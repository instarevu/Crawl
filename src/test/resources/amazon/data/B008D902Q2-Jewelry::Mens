<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.84;
        var shouldAutoPlay = false;
        var videoSizes = [[308, 445], [346, 500], [381, 550], [419, 606], [470, 679]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/91vNleSJK4L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51ihG5Q0DYL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51ihG5Q0DYL.jpg","main":{"http://ecx.images-amazon.com/images/I/91vNleSJK4L._SY445_.jpg":[308,445],"http://ecx.images-amazon.com/images/I/91vNleSJK4L._SY500_.jpg":[346,500],"http://ecx.images-amazon.com/images/I/91vNleSJK4L._SY550_.jpg":[381,550],"http://ecx.images-amazon.com/images/I/91vNleSJK4L._SY606_.jpg":[420,606],"http://ecx.images-amazon.com/images/I/91vNleSJK4L._SY679_.jpg":[470,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71yAmxkJh8L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/21AF4ieaFwL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/21AF4ieaFwL.jpg","main":{"http://ecx.images-amazon.com/images/I/71yAmxkJh8L._SX342_.jpg":[342,101],"http://ecx.images-amazon.com/images/I/71yAmxkJh8L._SX385_.jpg":[385,113],"http://ecx.images-amazon.com/images/I/71yAmxkJh8L._SX425_.jpg":[425,125],"http://ecx.images-amazon.com/images/I/71yAmxkJh8L._SX466_.jpg":[466,137],"http://ecx.images-amazon.com/images/I/71yAmxkJh8L._SX522_.jpg":[522,154]}}]}, 
                'colorToAsin': {'initial': {}},
                'holderRatio': 0.84,
                'holderMaxHeight': 700
                };
    A.trigger('P.AboveTheFold'); // trigger ATF event.    
    return data;    
});
