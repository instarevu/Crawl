try{
P.when('jQuery','A', 'cf').register('warrantyForesterLogging', function($,A){
  var watchWarranty = $("#warranty-div");
  var protocol = window.location.protocol == "https:" ? "https://" : "http://";
  var urlVip = watchWarranty.attr("data-foresterVip");
  var urlPrefix = watchWarranty.attr("data-foresterUrl");
  var params = {
    marketplaceId:watchWarranty.attr("data-marketplaceId"),
    requestId:watchWarranty.attr("data-requestId"),
    session:watchWarranty.attr("data-session")
  };
  var urlEndParameters = $.param(params);
  var warrantyType = watchWarranty.find("#watch-warranty-label").attr("data-warrantyType");
 
  return {
    foresterLog : function(data){
      var url = protocol + urlVip + urlPrefix + data + '?' + urlEndParameters;
      (new Image()).src = url;
    },
    warrantyType : warrantyType
  }; 
});
} catch(err){}
P.when('jQuery', 'warrantyForesterLogging').execute(function($, forester) {
  var watchWarranty = $("#warranty-div");
  var warrantyFileLink = watchWarranty.find("#watch-warranty-filelink");
  var warrantyPopover = watchWarranty.find("#watchWarrantyPopover");
  var warrantyLinkInPopover = $("#linkInPopover");

  if(warrantyFileLink.length) {
    forester.foresterLog("shown");
  }

  warrantyFileLink.click(function() {
    forester.foresterLog("clickedFromDP");
  });

  warrantyPopover.one('mouseenter',function(){
    forester.foresterLog("popoverShown" + forester.warrantyType);
  });

  warrantyLinkInPopover.click(function() {
    forester.foresterLog("clickedFromPopover" + forester.warrantyType);
  });
});
