    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/81BIZS1e4LL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41nDf62LCQL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/41nDf62LCQL.jpg","main":{"http://ecx.images-amazon.com/images/I/81BIZS1e4LL._SY395_.jpg":[325,395],"http://ecx.images-amazon.com/images/I/81BIZS1e4LL._SY500_.jpg":[412,500],"http://ecx.images-amazon.com/images/I/81BIZS1e4LL._SY535_.jpg":[441,535],"http://ecx.images-amazon.com/images/I/81BIZS1e4LL._SY575_.jpg":[474,575]}},{"hiRes":"http://ecx.images-amazon.com/images/I/713k2ILHnVL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41unOIbbKDL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/41unOIbbKDL.jpg","main":{"http://ecx.images-amazon.com/images/I/713k2ILHnVL._SY395_.jpg":[154,395],"http://ecx.images-amazon.com/images/I/713k2ILHnVL._SY500_.jpg":[195,500],"http://ecx.images-amazon.com/images/I/713k2ILHnVL._SY535_.jpg":[209,535],"http://ecx.images-amazon.com/images/I/713k2ILHnVL._SY575_.jpg":[224,575]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71zMW1FUZ8L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/31h0GobCZPL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/31h0GobCZPL.jpg","main":{"http://ecx.images-amazon.com/images/I/71zMW1FUZ8L._SY395_.jpg":[150,395],"http://ecx.images-amazon.com/images/I/71zMW1FUZ8L._SY500_.jpg":[191,500],"http://ecx.images-amazon.com/images/I/71zMW1FUZ8L._SY535_.jpg":[204,535],"http://ecx.images-amazon.com/images/I/71zMW1FUZ8L._SY575_.jpg":[219,575]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81OJFZpnymL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41AC5C1QNfL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/41AC5C1QNfL.jpg","main":{"http://ecx.images-amazon.com/images/I/81OJFZpnymL._SY395_.jpg":[365,395],"http://ecx.images-amazon.com/images/I/81OJFZpnymL._SY500_.jpg":[462,500],"http://ecx.images-amazon.com/images/I/81OJFZ