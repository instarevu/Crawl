<div id="buybox" class="a-section a-spacing-medium">
    <form method="post" id="addToCart" action="/gp/product/handle-buy-box/ref=dp_start-bbf_1_glance" class="a-content">
        <input type="hidden" id="session-id" name="session-id" value="000-0000000-0000000"> 
