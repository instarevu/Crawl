<script type="text/javascript">

function getSpecialOffersClipCouponResult() {
    jQuery.ajax({
        type: 'GET',
        url: '/gp/promotions/vpc?ie=UTF8&promoId=A1GYEBQKYO17FW&source=dp_so_B00DDMJ0JE',
        success: displaySpecialOffersClipCouponResult
    });
}

var specialOffersClickedClipCoupon = false; 

function displaySpecialOffersClipCouponResult(result) {
    if (!specialOffersClickedClipCoupon) {
        result = result.replace(/^\s+|\s+$/g,"");
        if (result == "SUCCESS" || result == "REDEEMED") {
            document.getElementById("productPromotion_clipped").innerHTML = decodeURIComponent("You%20have%20clipped%20this%20coupon.%20Your%20%242.00%20discount%20will%20be%20applied%20at%20checkout.");
        } else if (result == "EXCEEDED") {
            document.getElementById("productPromotion_clipped").innerHTML = decodeURIComponent("You%20have%20exceeded%20the%20quantity%20limit%20of%20this%20coupon.");
        } else {
            document.getElementById("productPromotion_clipped").innerHTML = decodeURIComponent("Sorry.%20You%20are%20not%20eligible%20for%20this%20coupon.");
        }
        specialOffersClickedClipCoupon = true;
        if(typeof displayClipCouponResult === 'function') {
            displayClipCouponResult(result);
        }
    }
}
</script>
