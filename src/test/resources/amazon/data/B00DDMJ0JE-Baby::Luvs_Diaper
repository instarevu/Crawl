<script type="text/javascript">
    var displayClipCouponResult;

    P.when('jQuery','ready').execute(function($) {
        var clickedClipCoupon = false;
        

        var isBuyBoxSns = false;
        
            isBuyBoxSns = true;
        

        var errorInClipping = false;

        displayClipCouponResult = function(result) {
            if (!clickedClipCoupon) {
                if (result.indexOf("SUCCESS") > -1 || result.indexOf("REDEEMED") > -1) {
                    if (isBuyBoxSns) {
                        $('#clippedCouponSns').show();
                        $('#unclippedCouponSns, #unclippedCoupon, #errorInClipping').hide();
                    } else {
                        $('#clippedCoupon').show();
                        $('#unclippedCoupon, #unclippedCouponSns, #errorInClipping').hide();
                    }
                } else {
                    $('#errorInClipping').show();
                    $('#unclippedCoupon, #clippedCoupon, #unclippedCouponSns, #clippedCouponSns').hide();
                    errorInClipping = true;
                }
                clickedClipCoupon = true;
                if (typeof displaySpecialOffersClipCouponResult === 'function') {
                    displaySpecialOffersClipCouponResult(result);
                }
            }
        };

        showOneTimeBuyVpcPriceBlock = function() {
            if (!errorInClipping) {
                if (clickedClipCoupon) {
                    $('#clippedCoupon').show();
                    $('#clippedCouponSns').hide();
                } else {
                    $('#unclippedCoupon').show();
                    $('#unclippedCouponSns').hide();
                }
            }
            isBuyBoxSns = false;
        };

        showSnsVpcPriceBlock = function() {
            if (!errorInClipping) {
                if (clickedClipCoupon) {
                    $('#clippedCouponSns').show();
                    $('#clippedCoupon').hide();
                } else {
                    $('#unclippedCouponSns').show();
                    $('#unclippedCoupon').hide();
                }
            }
            isBuyBoxSns = true;
        };

        clipCoupon = function(event) {
            
                
                
                    window.location = '/gp/sign-in.html/ref=sr_1_1?ie=UTF8&email=&redirectASIN=B00DDMJ0JE&disableCorpSignUp=&path=%2Fgp%2Fcoupon%2Fc%2FA1GYEBQKYO17FW&redirectProtocol=&mode=&useRedirectOnSuccess=1';
                    event.preventDefault();
                    return true;
                
            
        };

        $("#vpcButton").find(".a-button").click(clipCoupon);
        $("#vpcButtonSns").find(".a-button").click(clipCoupon);
    })
</script>
    


    
    
        
            
                
                
            
            
        
        
        
    


