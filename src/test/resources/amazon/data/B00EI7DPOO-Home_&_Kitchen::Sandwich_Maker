<script language="JavaScript">
<!--

function cv_mo1UADKR884OET9newHeight(h)
{	
    fp_resizePlayerSpace('cv_mo1UADKR884OET9', 0, h);
}

function cv_mo1UADKR884OET9resetPlayer(w,h,bw,cc) {
  cv_mo1UADKR884OET9newHeight(h);

  cv_mo1UADKR884OET9so.addVariable("resetBandwidth", bw);
  cv_mo1UADKR884OET9so.addVariable("cacheCode", cc);
  cv_mo1UADKR884OET9so.setAttribute("height", h);
  cv_mo1UADKR884OET9so.setAttribute("width", w);
  cv_mo1UADKR884OET9so.write("cv_mo1UADKR884OET9flashcontent");
}

