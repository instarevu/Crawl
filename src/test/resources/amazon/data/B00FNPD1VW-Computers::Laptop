<div id="servicesInterstitial_feature_div" class="feature" data-feature-name="servicesInterstitial">

    
        
        
            
            
                




    <span class="a-declarative" data-action="a-modal" data-a-modal="{&quot;name&quot;:&quot;servicesInterstitial&quot;,&quot;url&quot;:&quot;/gp/si/aui-si-pop.html?ie=UTF8&amp;ri=0QPHT9XFJWQ5MCWWQ5XP&amp;si=0&quot;,&quot;header&quot;:&quot;Protect Your Purchase&quot;}" id="servicesInterstitial"></span>
    <script type="a-state" data-a-state="{&quot;key&quot;:&quot;PSDServicesInterstitial&quot;}">{"showInstall":"false","refTag":"pd_si_147_"}</script>

<script type='text/javascript'>
amznJQ.addLogical('wcs-json',['http://z-ecx.images-amazon.com/images/G/01/browser-scripts/wcs-json/wcs-json-4084738007._V1_.js']);
amznJQ.onReady('wcs-json', function(){
  P.when('A','jQuery','a-popover-base').execute(function (A,$,base) {

      //Twister agnostic vars
      var siState = A.state("PSDServicesInterstitial");
      var showInstall = siState && siState['showInstall'] === "true";
      var refTag = siState['refTag'] || "pd_si_";

      var atcButton;
      var atcForm;
      var serviceASIN;
      var baseASIN;
      var index;

      var safeATCClick = function() {
      atcButton.unbind('click.siatc')                                           //Unbind the SI handler
               .click()                                                         //Trigger the ATC form submission
               .bind('click.siatc',function(){return false});                   //Prevent additional clicks on the ATC button
      };

      //SI Post-Popover Display JS
      var logRefTag = function(message) {
        $.get('/gp/product/ajax-handlers/reftag.html/ref=' + message);
      }

      var bindAddHandler = function(event) {
        unbindButtonHandlers();
        logRefTag(refTag+"y_"+baseASIN+"_"+serviceASIN);
        var offeringId = $('#siAddCoverage button').attr('data-offering-id');
        var quantity = document.getElementById('quantity');
        quantity = quantity === null ? 1 : quantity.value;
        if(typeof(offeringId) !== 'undefined'){
          var atcForm = $('#addToCart');
          atcForm.append('<input type="hidden" id="useABBHandler" name="useABBHandler" value="1" />');
          atcForm.append('<input type="hidden" id="quantity.'+index+'" name="quantity.'+index+'" value="'+quantity+'" />');
          atcForm.append('<input type="hidden" id="asin.'+index+'" name="asin.'+index+'" value="'+serviceASIN+'" />');
          atcForm.append('<input type="hidden" id="offeringID.'+index+'" name="offeringID.'+index+'" value="'+offeringId+'" />');

          var itemCount =  atcForm.find('input[name="itemCount"]');
          if(itemCount.length === 0){
            atcForm.append('<input type="hidden" name="itemCount" value="'+index+'">');
          }else{
            itemCount[0].value = index;
          }
        }

        safeATCClick();
        return false;
      }

      var bindNoHandler = function() {
        unbindButtonHandlers();
        logRefTag(refTag+"n_"+baseASIN+"_"+serviceASIN);
        safeATCClick();
        return false;
      }

      var unbindButtonHandlers = function() {
        $('#siAddCoverage').unbind('click.sipopadd');
        $('#siNoCoverage').unbind('click.sipopno');
        $('#siAddCoverage').bind('click.sipopadd', function(){return false});
        $('#siNoCoverage').bind('click.sipopno', function(){return false});
      }

      //Delegate click SI click handlers to body for when popover appears
      $(document.body).delegate('#siAddCoverage','click.sipopadd', bindAddHandler);
      $(document.body).delegate('#siNoCoverage','click.sipopno', bindNoHandler);

      //SI Pre-Popover Display JS
      var siATCHandler = function(){
        atcButton = $('#add-to-cart-button');
        atcForm = $('#addToCart');

        $(document.body).undelegate('#add-to-cart-button','click.siatc')
        if(atcButton.length === 1 && atcForm.length === 1){
          atcButton.bind('click.siatc',function(){return false;});  //prevent default ATC action

          index = atcForm.find("input[name^='offeringID']").length +1;
          //Find current base ASIN
          baseASIN = document.getElementById('ASIN');
          baseASIN = baseASIN === null ? null : baseASIN.value;

          //Find WBB ASINs
          var wbbASINs = '';
          var wbbChecks = $("#abbWrapper div").has("input[name^='offering']:checked");
          wbbChecks.find("input[name^='asin']")
                   .each(function(){ wbbASINs = this.value + ',' + wbbASINs; });

          var popData = $.parseJSON($('#servicesInterstitial').attr('data-a-modal'));
          var baseOfferID = document.getElementById('offerListingID').value;
          var merchantId = document.getElementById('merchantID').value; //TODO:Remove this when RW update has propagated.

          if(popData !== null && baseASIN !== null){

            //Bind default ATC to termination of popover
            A.on('a:popover:hide:servicesInterstitial', function(event){
                safeATCClick();
            });

            //Bind default ATC to AJAX failures
            A.on('a:popover:ajaxFail:servicesInterstitial', function(event){
                safeATCClick();
            });

            //Request service ASIN
            A.ajax('/gp/si/util/get-services.html', {
              method: 'get',
              params: {
                asin: baseASIN,
                merchantIdEnc: merchantId,
                offerID: baseOfferID,
                limitForMAP: 1,
                abbASINs: wbbASINs
              },
              success: function(data){
                if(showInstall && data.otherServices && data.otherServices.length > 1 && typeof data.otherServices[0]){
                    serviceASIN = data.otherServices[0];
                }

                if(typeof(serviceASIN) === 'undefined' && data.services && data.services.length > 1 && data.services[0]){
                    serviceASIN = data.services[0];
                }

                var quantity = document.getElementById('quantity');
                quantity = quantity === null ? 1 : quantity.value;
                if(typeof(serviceASIN) !== 'undefined'){
                  popData.url +='&ca=' + serviceASIN +
                                '&ba=' + baseASIN +
                                '&i=' + index +
                                '&quantity=' + quantity;

                  var modalParams = {
                    "name" : popData.name,
                    "header" : popData.header,
                    "url" : popData.url,
                    "width" : "685"
                  };
                  $("#servicesInterstitial").attr("data-a-modal", JSON.stringify(modalParams)).click();
                }else{
                  safeATCClick();
                }
              },
              error: function(){
                  safeATCClick();
              }
            });

            return false;
          }
        }

        //If siATCHandler has not returned false by this point, the default behavior of ATC button will be triggered
      };

      $(document.body).delegate('#add-to-cart-button','click.siatc', siATCHandler);
  });
});


</script>


                    
    

</div>


        
        
    
        
        
        
        



        
        
    
        
        
        
        



        
        
    
</div>
         
<div id="leftCol" class="leftCol">
    
        
        
        
        




<div id="imageBlock_feature_div" class="feature" data-feature-name="imageBlock">

    
        
        
            
            
                













<div id="imageBlock" class="a-section">
    <div class="a-fixed-left-grid"><div class="a-fixed-left-grid-inner" style="padding-left:32px">
        
        
            <div id="altImages" class="a-fixed-left-grid-col a-col-left" style="width:40px;margin-left:-40px;_margin-left:-20px;float:left;">
                






    <div id="thumbs-image" class="a-hidden" customfunctionname="(function(id, state, onloadFunction){ P.when('A').execute(function(A){ A.trigger('image-block-twister-swatch-click', id, state, onloadFunction) }); });"></div>
    
    <ul class="a-nostyle a-button-list a-vertical a-spacing-top-micro">
        
        
            
                
                
                
                
                <li class="a-spacing-small item"><span class="a-list-item">
                    <span class="a-declarative" data-action="thumb-action" data-thumb-action="{&quot;index&quot;:&quot;0&quot;,&quot;thumbnailIndex&quot;:&quot;0&quot;,&quot;type&quot;:&quot;image&quot;}">
                        <span class="a-button a-button-selected a-button-thumbnail a-button-toggle"><span class="a-button-inner"><input class="a-button-input" type="submit"><span class="a-button-text">
                            
                       		    
                                
                                    <img alt="" src="http://ecx.images-amazon.com/images/I/41ofJFA6XLL._SS40_.jpg">
                                
                             
                        </span></span></span>
                    </span>
                </span></li>
                
                
            
        
            
                
                
                
                
                <li class="a-spacing-small item"><span class="a-list-item">
                    <span class="a-declarative" data-action="thumb-action" data-thumb-action="{&quot;index&quot;:1,&quot;thumbnailIndex&quot;:1,&quot;type&quot;:&quot;image&quot;}">
