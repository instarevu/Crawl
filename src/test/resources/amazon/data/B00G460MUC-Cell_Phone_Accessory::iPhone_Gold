    <input type="hidden" name="promise-item.B00G460MUC" value="1387857600" />
    <input type="hidden" name="promise-cutoff-time.B00G460MUC" value="1387501200" />
    <input type="hidden" name="promise-promised-shipoption.B00G460MUC" value="second" />
