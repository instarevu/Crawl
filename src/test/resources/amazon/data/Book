if (typeof amznJQ != "undefined") {
   amznJQ.available('swfobjectJS', function() { _ns_1JMAEVE8Y4C5G9Z9VW8M_6015_loadSwf(movieID, playerVersion); });
   return false;
} else {
   return _ns_1JMAEVE8Y4C5G9Z9VW8M_6015_loadSwf(movieID, playerVersion);
}
}


	
