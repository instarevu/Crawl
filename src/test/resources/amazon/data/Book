var ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so;	   


function ns_1JMAEVE8Y4C5G9Z9VW8M_6015_loadSwf_Core(swfLocation, swfWidth, swfHeight, flashVersion, bgColor, swfParams, flashParams, embedAttributes) {

  try {
      ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so = new SWFObject(swfLocation, "ns_1JMAEVE8Y4C5G9Z9VW8M_6015_AMPlayerProd", swfWidth, swfHeight, flashVersion, bgColor);	   

      for (var v in swfParams) {
        ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.addVariable(v, encodeURIComponent(swfParams[v]));
      }
      for (var v in flashParams) {
        ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.addParam(v, flashParams[v]);
      }
      for (var v in embedAttributes) {
        ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.setAttribute(v, embedAttributes[v]);
      }

      ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.addVariable("swfEmbedTime", new Date().getTime());

      var agt = navigator.userAgent;
       
      var reFirefox = new RegExp("firefox/", "i");
      var regx = agt.split(reFirefox);
      var ffVersion = 8;
      if (regx[1]) {
          var pts = regx[1].split(/[.]/);
          ffVersion = parseFloat(pts[0] + "." + pts[1]);
      }
      var minorVersion = parseFloat(navigator.ap);
      if ((navigator.appVersion.indexOf("Mac")!=-1) || (ffVersion < 1.5)) {
          ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.setAttribute("height", swfHeight);
          ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.setAttribute("width", swfWidth);
          ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.addVariable("oldFirefox", "1");	     
      }

      if (ns_1JMAEVE8Y4C5G9Z9VW8M_6015_so.write("ns_1JMAEVE8Y4C5G9Z9VW8M_6015_flashcontent")) {
          fp_resizePlayerSpace('ns_1JMAEVE8Y4C5G9Z9VW8M_6015_', 
                               320, 
                               283);
      }
      else {
          var nsPrefix = 'ns_1JMAEVE8Y4C5G9Z9VW8M_6015_';

          fp_resizePlayerSpace(nsPrefix, 0, 0);
          fp_rewriteDiv(nsPrefix, "flashcontent", '<br/>To view this video download <a target="_blank" href="http://www.macromedia.com/go/getflashplayer" target="_top">Flash Player</a> (version 9.0.115 or higher)');
      }
  } catch(err) {
      fp_resizePlayerSpace('ns_1JMAEVE8Y4C5G9Z9VW8M_6015_', 0, 0);
  }
  return false;
}



function _ns_1JMAEVE8Y4C5G9Z9VW8M_6015_loadSwf(movieID, playerVersion) {
    var swfLocation = "http://g-ecx.images-amazon.com/images/G/01/am3/20120510035744301/AMPlayer._V148501545_.swf";
    var swfWidth = "320";
    var swfHeight = "100%";
    var flashVersion = "9.0.115";
    var bgColor = "#FFFFFF";

    var swfParams = {};
    var flashParams = {};
    var embedAttributes = {};
    swfParams.amazonPort = "80";
    swfParams.allowFullScreen = "true";
    swfParams.nsPrefix = "ns_1JMAEVE8Y4C5G9Z9VW8M_6015_";
    swfParams.locale = "en_US";
    swfParams.logUrl = "gp/mpd/l";
    swfParams.canResize = "1";
    swfParams.autoPlay = "1";
    swfParams.sessionId = "000-0000000-0000000";
    swfParams.marketplaceID = "ATVPDKIKX0DER";
    swfParams.salign = "LT";
    swfParams.asin = "0385537131";
    swfParams.preset = "detail";
    swfParams.mediaObjectId = "m2QHB0VRGHRCDC";
    swfParams.autoPlayTimer = "";
    swfParams.mediaObjectIDList = "m2QHB0VRGHRCDC";
    swfParams.playIsUserGenerated = "1";
    swfParams.permUrl = "gp/mpd/permalink";
    swfParams.xmlUrl = "http://www.amazon.com/gp/mpd/getplaylist-v2/m2QHB0VRGHRCDC/000-0000000-0000000";
    swfParams.amazonServer = "www.amazon.com";
    swfParams.scale = "noscale";
    swfParams.amazonAmbLink = "/foo.html";
    flashParams.allowFullScreen = "true";
    flashParams.salign = "LT";
    flashParams.bgcolor = "#FFFFFF";
    flashParams.allowScriptAccess = "always";
    flashParams.quality = "high";
    flashParams.wmode = "transparent";
    embedAttributes.title = "Flash Player";

    if (movieID){
      if (playerVersion == '3'){
        swfLocation = "http://g-ecx.images-amazon.com/images/G/01/am3/20120510035744301/AMPlayer._V148501545_.swf";
        swfParams.mediaObjectIDList = movieID;
      } else {
        swfLocation = "http://g-ecx.images-amazon.com/images/G/01/am3/legacy/20101014901/AMPlayerProd._V197491872_.swf";
      }
      swfParams.xmlUrl = "http://www.amazon.com/gp/mpd/getplaylist-v2/" + movieID + "/000-0000000-0000000";
      flashParams.wmode = "transparent";
      swfParams.autoPlay = "1";
    }

    if (document.getElementById('ns_1JMAEVE8Y4C5G9Z9VW8M_6015_duration')) {
      document.getElementById('ns_1JMAEVE8Y4C5G9Z9VW8M_6015_duration').style.display='none';
    }
    ns_1JMAEVE8Y4C5G9Z9VW8M_6015_loadSwf_Core(swfLocation, swfWidth, swfHeight, flashVersion, bgColor, swfParams, flashParams, embedAttributes);

  return false;
}

