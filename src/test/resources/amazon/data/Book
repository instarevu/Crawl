<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3Cb%3E%3Ci%3EJohn%20Grisham%20takes%20you%20back%20to%20where%20it%20all%20began%20.%20.%20.%3C%2Fi%3E%3C%2Fb%3E%3Cbr%3E%3Cbr%3E%20John%20Grisham%27s%20%3Ci%3EA%20Time%20to%20Kill%3C%2Fi%3E%20is%20one%20of%20the%20most%20popular%20novels%20of%20our%20time.%20Now%20we%20return%20to%20that%20famous%20courthouse%20in%20Clanton%20as%20Jake%20Brigance%20once%20again%20finds%20himself%20embroiled%20in%20a%20fiercely%20controversial%20trial-a%20trial%20that%20will%20expose%20old%20racial%20tensions%20and%20force%20Ford%20County%20to%20confront%20its%20tortured%20history.%3Cbr%3E%3Cbr%3E%20Seth%20Hubbard%20is%20a%20wealthy%20man%20dying%20of%20lung%20cancer.%20He%20trusts%20no%20one.%20Before%20he%20hangs%20himself%20from%20a%20sycamore%20tree%2C%20Hubbard%20leaves%20a%20new%2C%20handwritten%2C%20will.%20It%20is%20an%20act%20that%20drags%20his%20adult%20children%2C%20his%20black%20maid%2C%20and%20Jake%20into%20a%20conflict%20as%20riveting%20and%20dramatic%20as%20the%20murder%20trial%20that%20made%20Brigance%20one%20of%20Ford%20County%27s%20most%20notorious%20citizens%2C%20just%20three%20years%20earlier.%3Cbr%3E%3Cbr%3E%20The%20second%20will%20raises%20far%20more%20questions%20than%20it%20answers.%20Why%20would%20Hubbard%20leave%20nearly%20all%20of%20his%20fortune%20to%20his%20maid%3F%20Had%20chemotherapy%20and%20painkillers%20affected%20his%20ability%20to%20think%20clearly%3F%20And%20what%20does%20it%20all%20have%20to%20do%20with%20a%20piece%20of%20land%20once%20known%20as%20Sycamore%20Row%3F%3Cbr%3E%3Cbr%3E%20In%20%3Ci%3ESycamore%20Row%3C%2Fi%3E%2C%20John%20Grisham%20returns%20to%20the%20setting%20and%20the%20compelling%20characters%20that%20first%20established%20him%20as%20America%27s%20favorite%20storyteller.%20Here%2C%20in%20his%20most%20assured%20and%20thrilling%20novel%20yet%2C%20is%20a%20powerful%20testament%20to%20the%20fact%20that%20Grisham%20remains%20the%20master%20of%20the%20legal%20thriller%2C%20nearly%20twenty-five%20years%20after%20the%20publication%20of%20%3Ci%3EA%20Time%20to%20Kill%3C%2Fi%3E.",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

                    
    

