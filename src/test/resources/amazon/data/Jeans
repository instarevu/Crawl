<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 444], [384, 500], [423, 550], [466, 605], [522, 678]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/91ITzQxmYHL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41pU7nF8IPL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41pU7nF8IPL.jpg","main":{"http://ecx.images-amazon.com/images/I/91ITzQxmYHL._SX342_.jpg":[342,445],"http://ecx.images-amazon.com/images/I/91ITzQxmYHL._SY500_.jpg":[385,500],"http://ecx.images-amazon.com/images/I/91ITzQxmYHL._SY550_.jpg":[423,550],"http://ecx.images-amazon.com/images/I/91ITzQxmYHL._SX466_.jpg":[466,606],"http://ecx.images-amazon.com/images/I/91ITzQxmYHL._SX522_.jpg":[522,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/91UjtLWWd9L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41PBO%2BZC1SL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41PBO%2BZC1SL.jpg","main":{"http://ecx.images-amazon.com/images/I/91UjtLWWd9L._SX342_.jpg":[342,445],"http://ecx.images-amazon.com/images/I/91UjtLWWd9L._SY500_.jpg":[385,500],"http://ecx.images-amazon.com/images/I/91UjtLWWd9L._SY550_.jpg":[423,550],"http://ecx.images-amazon.com/images/I/91UjtLWWd9L._SX466_.jpg":[466,606],"http://ecx.images-amazon.com/images/I/91UjtLWWd9L._SX522_.jpg":[522,679]}}]}, 
