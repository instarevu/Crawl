<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 1.0;
        var shouldAutoPlay = false;
        var videoSizes = [[338, 355], [428, 450], [425, 446], [466, 489], [522, 548]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/81C52ZEUWNL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/511YuoKewkL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/511YuoKewkL.jpg","main":{"http://ecx.images-amazon.com/images/I/81C52ZEUWNL._SY355_.jpg":[338,355],"http://ecx.images-amazon.com/images/I/81C52ZEUWNL._SY450_.jpg":[429,450],"http://ecx.images-amazon.com/images/I/81C52ZEUWNL._SX425_.jpg":[425,446],"http://ecx.images-amazon.com/images/I/81C52ZEUWNL._SX466_.jpg":[466,489],"http://ecx.images-amazon.com/images/I/81C52ZEUWNL._SX522_.jpg":[522,548]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81GmWyg1nDL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51fS7ADPE2L._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/51fS7ADPE2L.jpg","main":{"http://ecx.images-amazon.com/images/I/81GmWyg1nDL._SY355_.jpg":[337,355],"http://ecx.images-amazon.com/images/I/81GmWyg1nDL._SY450_.jpg":[427,450],"http://ecx.images-amazon.com/images/I/81GmWyg1nDL._SX425_.jpg":[425,447],"http://ecx.images-amazon.com/images/I/81GmWyg1nDL._SX466_.jpg":[466,491],"http://ecx.images-amazon.com/images/I/81GmWyg1nDL._SX522_.jpg":[522,550]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71a1fQIBO5L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/516t-R60Y%2BL._SS40_.jpg","large":"http://ecx.images-amazon.com/images/I/516t-R60Y%2BL.jpg","main":{"http://ecx.images-amazon.com/images/I/71a1fQIBO5L._SY355_.jpg":[333,355],"http://ecx.images-amazon.com/images/I/71a1fQIBO5L._SY450_.jpg":[422,450],"http://ecx.images-amazon.com/images/I/71a1fQIBO5L._SX425_.jpg":[425,453],"http://ecx.images-amazon.com/images/I/71a1fQIBO5L._SX466_.jpg":[466,497],"http://ecx.images-amazon.com/images/I/71a1fQIBO5L._SX522_.jpg":[522,556]}}]}, 
                'colorToAsin': {'initial': {}},
                'holderRatio': 1.0,
                'holderMaxHeight': 700
                };
    A.trigger('P.AboveTheFold'); // trigger ATF event.    
    return data;    
});
</script>

                    
    

