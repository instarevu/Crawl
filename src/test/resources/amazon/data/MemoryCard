function cv_mo135JKQ6Y5T3DIloadSwf_Core(swfLocation, swfWidth, swfHeight, flashVersion, bgColor, swfParams, flashParams, embedAttributes) {

  try {
      cv_mo135JKQ6Y5T3DIso = new SWFObject(swfLocation, "cv_mo135JKQ6Y5T3DIAMPlayerProd", swfWidth, swfHeight, flashVersion, bgColor);	   

      for (var v in swfParams) {
        cv_mo135JKQ6Y5T3DIso.addVariable(v, encodeURIComponent(swfParams[v]));
      }
      for (var v in flashParams) {
        cv_mo135JKQ6Y5T3DIso.addParam(v, flashParams[v]);
      }
      for (var v in embedAttributes) {
        cv_mo135JKQ6Y5T3DIso.setAttribute(v, embedAttributes[v]);
      }

      cv_mo135JKQ6Y5T3DIso.addVariable("swfEmbedTime", new Date().getTime());

      var agt = navigator.userAgent;
       
      var reFirefox = new RegExp("firefox/", "i");
      var regx = agt.split(reFirefox);
      var ffVersion = 8;
      if (regx[1]) {
          var pts = regx[1].split(/[.]/);
          ffVersion = parseFloat(pts[0] + "." + pts[1]);
      }
      var minorVersion = parseFloat(navigator.ap);
      if ((navigator.appVersion.indexOf("Mac")!=-1) || (ffVersion < 1.5)) {
          cv_mo135JKQ6Y5T3DIso.setAttribute("height", swfHeight);
          cv_mo135JKQ6Y5T3DIso.setAttribute("width", swfWidth);
          cv_mo135JKQ6Y5T3DIso.addVariable("oldFirefox", "1");	     
      }

      if (cv_mo135JKQ6Y5T3DIso.write("cv_mo135JKQ6Y5T3DIflashcontent")) {
          fp_resizePlayerSpace('cv_mo135JKQ6Y5T3DI', 
