    <span class="a-declarative" data-action="a-modal" data-a-modal="{}" id=size-chart-popover>
        <a id="size-chart-url" data-header="Size Chart" class="a-size-small a-link-normal" target="_blank" href="/gp/product/ajax-handlers/apparel-sizing-chart.html/ref=dp_sizechart?ie=UTF8&asin=B00D7CHAEW">
            Sizing info</a>
    </span>
    
    
    

<script type=text/javascript>
P.when("jQuery", "cf").execute(function($){
    
	
	P.when("A").execute(function(A) {
	    A.$("#fitRecommendationsLink").bind("click", function(event) {
	        event.preventDefault();
	    });
	});
	
	P.when("A").execute(function(A) {
        A.on("resize", function() {
            var $iframe = $("#fit-guide");
            var height = getIFrameHeight();
            if($iframe.length == 0) {
                var $trigger = $("#size-chart-popover");
                var data = $trigger.data("a-modal");
                var url = data.url;
                data.url = updateURLParameter(url, "height", height);
                $trigger.data("a-modal", data);
        	} else {
        	    if(!A.capabilities.ios) {
        	        $iframe.height(height);
        	    }
        	} 
        });
        
        
        function updateURLParameter(url, param, paramVal){
            var newAdditionalURL = "";
            var paramsArray = url.split("?");
            var baseURL = paramsArray[0];
            var additionalURL = paramsArray[1];
            var urlParamDelimiter = "";
            if (additionalURL) {
                paramsArray = additionalURL.split("&");
                var arrayLength = paramsArray.length;
                for (var i = 0; i < arrayLength; i++){
                    if(paramsArray[i].split("=")[0] != param){
                        newAdditionalURL += urlParamDelimiter + paramsArray[i];
                        urlParamDelimiter = "&";
                    }
                }
            }

            var modifiedParam = urlParamDelimiter + "" + param + "=" + paramVal;
            return baseURL + "?" + newAdditionalURL + modifiedParam;
        }
        
        function getIFrameHeight(){
            var avail =  A.viewport().height - 300;
            var height = Math.min(Math.max(avail, 300), 700);
            return height;
        }
        
        function setDeclarativeActionParams() {
            var link = $("#size-chart-url");
            var popover_header = link.attr("data-header");
            var popover_url = link.attr("href") + "&height=" + getIFrameHeight() +"&isUDP=1";
            
            var popoverActionParams = '{"name" : "sizeChart", "width" : "800", "header" : "' + popover_header + '", "url" : "' + popover_url + '"}';        
            $("#size-chart-popover").attr("data-a-modal", popoverActionParams);
        }
        
        setDeclarativeActionParams();
    });
       
});
</script>

	                
	                
	                
	            
	            
	            
	        </span>
	    
	    
		    
		    <noscript><input type="image" src="http://g-ecx.images-amazon.com/images/G/01/buttons/update-page._V192192236_.gif" alt="Update Button" name="submit.twisterUpdateButton_0"/></noscript>
		    <input type="button" id="twisterUpdate_size_name" name="submit.twisterUpdateButton_0" class="a-hidden"/>
		
		
	
	
</div>



            
            
        
    
        
        
            
            
                
                    
                    	





<div id="variation_color_name" class="a-section a-spacing-small">
    <div class="a-row">
        <label>
            Color:
        </label>
        <span class="selection">
            
                Walnut
            
        </span>
    </div>
    
    

    
    
    <ul class="a-nostyle a-button-list a-horizontal a-spacing-top-micro swatches swatchesSquare imageSwatches">
        
            
            
            
            
            
            
            
             
             
