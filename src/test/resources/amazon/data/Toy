    return cv_mo10EXLYKKK9DHXloadSwf();
  }
  
  var node = cv_mo10EXLYKKK9DHXpreplay.parentNode;

  var cv_mo10EXLYKKK9DHXcurrentClick = node.onclick;
  node.onclick = function () {
    var head = document.getElementsByTagName("head");
