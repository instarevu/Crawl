    function cdBindAskPopover() {
      jQuery('#AskPopover').each(function() {
        jQuery(this).removeAmazonPopoverTrigger();
        jQuery(this).amazonPopoverTrigger({
          ajaxTimeout: 4000,
          ajaxErrorContent: jQuery(this).attr('popover-error-string'),
          modal: true,
          width: 800,
          closeEventInclude: "MOUSE_LEAVE",
          closeEventExclude: ["MOUSE_LEAVE", "CLICK_OUTSIDE"],
          showCloseButton: true,
          showOnHover: false,
          title: jQuery(this).attr('popover-title'),
          cacheable: false,
          folllowScroll: true,
          destination: '/gp/product/features/popover/ask-popover.html?sourceWidget=AskATFDPX&asin=' + jQuery(this).attr('data-asin')
        });
      });
    }
  
    amznJQ.onReady('popover', cdBindAskPopover);
