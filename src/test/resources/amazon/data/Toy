      logTag.src = "/gp/customer-media/du/log.html?ie=UTF8&p=CustomerImage&s=CustomerVideoPlay";
      head[0].appendChild(logTag);
    }

    if (cv_mo10EXLYKKK9DHXcurrentClick) {
      return cv_mo10EXLYKKK9DHXcurrentClick();
    }
  }
}

