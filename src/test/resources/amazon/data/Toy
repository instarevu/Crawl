    <input type="hidden" name="promise-item.B0053X62GK" value="1387339200" />
    <input type="hidden" name="promise-cutoff-time.B0053X62GK" value="1387223100" />
    <input type="hidden" name="promise-promised-shipoption.B0053X62GK" value="next" />
